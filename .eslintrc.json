{
  "env": {
    "node": true,
    "es2021": true,
    "jest": true,
    "browser": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {}
  },
  "rules": {
    "no-console": ["warn", { "allow": ["warn", "error", "info"] }]
  }
}
